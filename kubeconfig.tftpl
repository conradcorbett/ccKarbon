apiVersion: v1
kind: Config
clusters:
- name: ${karbon_cluster_name}
  cluster:
    server: ${cluster_url}
    certificate-authority-data: ${cluster_ca_certificate}
users:
- name: default-user-${karbon_cluster_name}
  user:
    token: ${access_token}
contexts:
- context:
    cluster: ${karbon_cluster_name}
    user: default-user-${karbon_cluster_name}
  name: ${karbon_cluster_name}-context
current-context: ${karbon_cluster_name}-context

